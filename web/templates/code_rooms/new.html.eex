<%= form_for @changeset, code_rooms_path(@conn, :create), fn f -> %>
  <label>
    Name: <%= text_input f, :name %>
  </label>
  <label>
    Language: <%= select f, :language, ["Ruby"] %>
  </label>
  <label>
    Private: <%= select f, :private, [true] %>
  </label>
  <%= submit "Submit" %>
<% end %>
